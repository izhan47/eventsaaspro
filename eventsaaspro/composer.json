{
    "name": "classiebit/eventsaaspro-pro",
    "version": "2.0.0",
    "description": "Run your own Events business with EventSaaSPro. Use it as event ticket selling website or event management platform on your own domain.",
    "keywords": [
        "online events",
        "zoom",
        "google meet",
        "jio meet",
        "events",
        "event ticket selling website",
        "event management platform",
        "event-management",
        "event-planning",
        "laravel",
        "laravel-package",
        "voyager",
        "vuejs"
    ],
    "license": "Commercial",
    "homepage": "https://classiebit.com/eventsaaspro-pro",

    "require": {
        "illuminate/support": "~8.0|~9.0",
        "consoletvs/charts": "6.*",
        "barryvdh/laravel-dompdf": "^2.0",
        "laravel/socialite": "5.*",
        "simplesoftwareio/simple-qrcode": "4.*",
        "tcg/voyager": "1.5.*",
        "tightenco/ziggy": "1.*",
        "usmanhalalit/laracsv": "2.*",
        "yajra/laravel-datatables-oracle": "9.*",
        "league/omnipay": "3.*",
        "omnipay/paypal": "^3.0",
        "spatie/laravel-honeypot": "*",
        "laravel/sanctum": "2.*",
        "ladumor/laravel-pwa": "^0.0.3",
        "stripe/stripe-php": "^13.4"
    },
    "require-dev": {
        "laravel/framework": "~8.0|~9.0"
    },
    "autoload": {
        "psr-4": {
            "Eventsaaspro\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Eventsaaspro\\EventmieServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
